<div class="authform">

  <%= form_for(resource, :as => resource_name,  :url => registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
      <h3 class="text-center">Sign up</h3>
      <div class ="container">
    <div class="form-group">
    <%= f.email_field :email, :autofocus => true, :placeholder => 'Email address', class: 'form-control' %>
  </div>

  <div class="form-group">
  <%= f.text_field :house_name, :autofocus => true, :placeholder => 'Name of your house', class: 'form-control' %>
</div>

  <div class="form-group">
    <%= f.password_field :password, :placeholder => 'Passwordbruh', class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, :placeholder => 'Password', class: 'form-control' %>
  </div>

    <%= f.submit "Sign up",:class=>'btn btn-primary' %>

    <%= render "devise/shared/links" %>
  <% end %>

</div>
</div>
